    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@types/browser-sync": "0.0.32",
    "@types/fs-extra": "0.0.31",
    "@types/jasmine": "^2.2.33",
    "@types/lodash": "^4.14.34",
    "@types/node": "^6.0.38",
    "@types/node-sass": "0.0.28",
    "browser-sync": "^2.14.0",
    "chai": "^3.5.0",
    "chokidar": "^1.6.0",
    "codelyzer": "0.0.28",
    "connect-history-api-fallback": "^1.3.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.0",
